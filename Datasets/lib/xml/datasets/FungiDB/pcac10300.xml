<datasets>
  <constant name="organismAbbrev" value="pcac10300"/>
  <constant name="strainAbbrev" value="10300"/>
  <constant name="referenceStrainOrganismAbbrev" value="pcac10300"/>
  <constant name="familyRepOrganismAbbrev" value="pinfT30-4"/>
  <constant name="projectName" value="FungiDB"/>
  <constant name="ncbiTaxonId" value="9000000727"/>
  <constant name="speciesNcbiTaxonId" value="29920"/>
  <constant name="soTerm" value="contig"/>
  <constant name="genomeSource" value="GenBank"/>
  <constant name="genomeVersion" value="2018-07-03"/>
  <constant name="source" value="GenBank"/>
  <constant name="functAnnotVersion" value="2018-07-03"/>
  <constant name="ebiOrganismName" value="pcac10300"/>

  <dataset class="validateOrganismInfo">
    <prop name="projectName">$$projectName$$</prop>
    <prop name="organismAbbrev">$$organismAbbrev$$</prop>
    <prop name="strainAbbrev">$$strainAbbrev$$</prop>
    <prop name="ncbiTaxonId">$$ncbiTaxonId$$</prop>
    <prop name="speciesNcbiTaxonId">$$speciesNcbiTaxonId$$</prop>
    <prop name="genomeVersion">$$genomeVersion$$</prop>
  </dataset>

  <dataset class="ebi_primary_genome">
    <prop name="genomeVersion">$$genomeVersion$$</prop>
    <prop name="ebi2gusTag">111</prop>
    <prop name="projectName">$$projectName$$</prop>
    <prop name="organismAbbrev">$$organismAbbrev$$</prop>
    <prop name="ncbiTaxonId">$$ncbiTaxonId$$</prop>
    <prop name="ebiOrganismName">$$ebiOrganismName$$</prop>
    <prop name="ebiVersion">build_68</prop>
    <prop name="releaseDate"></prop>
  </dataset>

  <dataset class="ECAssocFromUniprot">
    <prop name="speciesNcbiTaxonId">$$speciesNcbiTaxonId$$</prop>
    <prop name="organismAbbrev">$$organismAbbrev$$</prop>
    <prop name="version">TODAY</prop>
  </dataset>

  <dataset class="referenceStrain-GenbankEST">
    <prop name="projectName">$$projectName$$</prop>
    <prop name="organismAbbrev">$$organismAbbrev$$</prop>
    <prop name="ncbiTaxonId">$$ncbiTaxonId$$</prop>
  </dataset>

  <dataset class="referenceStrain-dbEST">
    <prop name="projectName">$$projectName$$</prop>
    <prop name="organismAbbrev">$$organismAbbrev$$</prop>
    <prop name="speciesNcbiTaxonId">$$speciesNcbiTaxonId$$</prop>
  </dataset>

  <dataset class="transcriptsFromReferenceStrain">
    <prop name="referenceStrainOrganismAbbrev">$$referenceStrainOrganismAbbrev$$</prop>
  </dataset>



</datasets>
