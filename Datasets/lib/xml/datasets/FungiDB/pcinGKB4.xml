<datasets>
  <constant name="organismAbbrev" value="pcinGKB4"/>
  <constant name="strainAbbrev" value="GKB4"/>
  <constant name="referenceStrainOrganismAbbrev" value="pcinGKB4"/>
  <constant name="familyRepOrganismAbbrev" value="pinfT30-4"/>
  <constant name="projectName" value="FungiDB"/>
  <constant name="ncbiTaxonId" value="9000000705"/>
  <constant name="speciesNcbiTaxonId" value="4785"/>
  <constant name="soTerm" value="contig"/>
  <constant name="genomeSource" value="GenBank"/>
  <constant name="genomeVersion" value="2021-06-03"/>
  <constant name="source" value="GenBank"/>
  <constant name="functAnnotVersion" value="2021-06-03"/>
  <constant name="ebiOrganismName" value="pcinGKB4"/>

  <dataset class="validateOrganismInfo">
    <prop name="projectName">$$projectName$$</prop>
    <prop name="organismAbbrev">$$organismAbbrev$$</prop>
    <prop name="strainAbbrev">$$strainAbbrev$$</prop>
    <prop name="ncbiTaxonId">$$ncbiTaxonId$$</prop>
    <prop name="speciesNcbiTaxonId">$$speciesNcbiTaxonId$$</prop>
    <prop name="genomeVersion">$$genomeVersion$$</prop>
  </dataset>

  <dataset class="ebi_primary_genome">
    <prop name="genomeVersion">$$genomeVersion$$</prop>
    <prop name="ebi2gusTag">107</prop>
    <prop name="projectName">$$projectName$$</prop>
    <prop name="organismAbbrev">$$organismAbbrev$$</prop>
    <prop name="ncbiTaxonId">$$ncbiTaxonId$$</prop>
    <prop name="ebiOrganismName">$$ebiOrganismName$$</prop>
    <prop name="ebiVersion">build_61</prop>
    <prop name="releaseDate"></prop>
  </dataset>

  <dataset class="dbxref_gene2Entrez">
    <prop name="projectName">$$projectName$$</prop>
    <prop name="organismAbbrev">$$organismAbbrev$$</prop>
    <prop name="ncbiTaxonId">$$ncbiTaxonId$$</prop>
    <prop name="genomeVersion">$$genomeVersion$$</prop>
    <prop name="version">2025-04-08</prop>
  </dataset>

  <dataset class="dbxref_gene2PubmedFromNcbi">
    <prop name="projectName">$$projectName$$</prop>
    <prop name="organismAbbrev">$$organismAbbrev$$</prop>
    <prop name="ncbiTaxonId">$$ncbiTaxonId$$</prop>
    <prop name="genomeVersion">$$genomeVersion$$</prop>
    <prop name="version">2025-04-08</prop>
  </dataset>

  <dataset class="ECAssocFromUniprot">
    <prop name="speciesNcbiTaxonId">$$speciesNcbiTaxonId$$</prop>
    <prop name="organismAbbrev">$$organismAbbrev$$</prop>
    <prop name="version">TODAY</prop>
  </dataset>

  <dataset class="referenceStrain-GenbankEST">
    <prop name="projectName">$$projectName$$</prop>
    <prop name="organismAbbrev">$$organismAbbrev$$</prop>
    <prop name="ncbiTaxonId">$$ncbiTaxonId$$</prop>
  </dataset>

  <dataset class="referenceStrain-dbEST">
    <prop name="projectName">$$projectName$$</prop>
    <prop name="organismAbbrev">$$organismAbbrev$$</prop>
    <prop name="speciesNcbiTaxonId">$$speciesNcbiTaxonId$$</prop>
  </dataset>

  <dataset class="transcriptsFromReferenceStrain">
    <prop name="referenceStrainOrganismAbbrev">$$referenceStrainOrganismAbbrev$$</prop>
  </dataset>


  <dataset class="ebiRnaSeqExperiment">
    <prop name="projectName">$$projectName$$</prop>
    <prop name="organismAbbrev">$$organismAbbrev$$</prop>
    <prop name="name">Engelbrecht_GBK4RNA_2021</prop>
    <prop name="version">2021-04-26_fromEbi</prop>
    <prop name="isStrandSpecific">true</prop>
  </dataset>

  <dataset class="ebiRnaSeqExperiment">
    <prop name="projectName">$$projectName$$</prop>
    <prop name="organismAbbrev">$$organismAbbrev$$</prop>
    <prop name="name">Engelbrecht_GBK4RNA_2021_un</prop>
    <prop name="version">2021-04-26_fromEbi</prop>
    <prop name="isStrandSpecific">false</prop>
  </dataset>

</datasets>
